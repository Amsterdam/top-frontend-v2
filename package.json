{
  "name": "top-frontend-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "acc": "env-cmd -f .env.acceptance vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest",
    "generate:api-schema:local": "openapi-typescript http://localhost:8000/api/v1/schema/ -o ./src/__generated__/apiSchema.d.ts",
    "generate:api-schema:acc": "openapi-typescript https://acc.api.top.amsterdam.nl/api/v1/schema/ -o ./src/__generated__/apiSchema.d.ts"
  },
  "dependencies": {
    "@amsterdam/design-system-assets": "^1.1.0",
    "@amsterdam/design-system-css": "^2.2.0",
    "@amsterdam/design-system-react": "^2.2.0",
    "@amsterdam/design-system-react-icons": "^1.1.1",
    "@amsterdam/design-system-tokens": "^2.2.0",
    "@amsterdam/ee-ads-rhf-lib": "file:amsterdam-ee-ads-rhf-lib-0.0.0.tgz",
    "env-cmd": "^11.0.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-oidc-context": "^3.3.0",
    "react-router": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
